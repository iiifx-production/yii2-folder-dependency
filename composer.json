    "authors": [
        { "name": "Vitaliy IIIFX Khomenko", "email": "iiifx@yandex.com" }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": { "iiifx\\cache\\dependency\\": "source/" }
    }
